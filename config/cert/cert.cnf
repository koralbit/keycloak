[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = req_distinguished_name
req_extensions      = req_ext

[req_distinguished_name]
C  = MX
ST = Mexico City
L  = Mexico City
O  = Keycloak
OU = IT
CN = *.local-keycloak.mx


[ req_ext ]
subjectAltName      = @alt_names

[ alt_names ]
DNS.1               = *.local-keycloak.mx
DNS.2               = local-keycloak.mx